<link rel="stylesheet" href="/css/userSignIn.css">

<main>
    <section class="loginForm">
        <h2>Register</h2>
        <form action="/user/registerUser" method="POST"> 
            <div class="inputField">
                <input type="text" name="firstName" id="inputFirstName" placeholder=" ">
                <label for="inputFirstName">First Name</label>
            </div>
            <div class="inputField">
                <input type="text" name="lastName" id="inputLastName" placeholder=" ">
                <label for="inputLastName">Last Name</label>
            </div>
            <div class="inputField">
                <input type="email" name="email" id="inputEmail" placeholder=" ">
                <label for="inputEmail">Email</label>
            </div>
            <div class="inputField">
                <input type="password" name="password" id="inputPassword" placeholder=" ">
                <label for="inputPassword">Password</label>
            </div>
            <button type="submit" id="login-btn">
                Register
            </button>
        </form>
